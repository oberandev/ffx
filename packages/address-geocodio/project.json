{
  "name": "address-geocodio",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/address-geocodio/src",
  "projectType": "library",
  "targets": {
    "build": {
      "command": "packages/address-geocodio/build"
    },
    "format:check": {
      "command": "prettier ./**/*.{mts,ts} --check"
    },
    "lint": {
      "command": "eslint packages/address-geocodio/**/*.{js,mts,ts}"
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs address-geocodio {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "coverage": true,
        "passWithNoTests": false,
        "reporters": ["json"],
        "reportsDirectory": "../../coverage/packages/address-geocodio"
      },
      "configurations": {
        "watch": {
          "watch": true
        }
      }
    }
  },
  "tags": []
}
